extends layout

block content
  div.container
    h1.muted= title
    include navbar
    div.row
      div.span12
        //img.img-rounded(data-src="holder.js/940x300")
        img.img-rounded(src="/img/superint-home.jpg")

    paragraph = {headline:pageContent.headline, lines:pageContent.paragraphs}
    include paragraph

    h3.text-info= pageContent.dev0
    each dev in pageContent.devs
        li= dev
