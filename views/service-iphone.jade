extends layout

block content
  div.container
    h1.muted= title
    include navbar
    div.row
      div.span12
        //img.img-rounded(data-src="holder.js/940x300")
        img.img-rounded(src="/img/joyo-home.jpg")

    ul.thumbnails
      li.span3
        a.thumbnail
          img(src="/img/apple_logo.jpg", style="height:135px;")
      li.span3
        a.thumbnail
          img(src="/img/android_logo.jpg", style="height:135px;")
      li.span3
        a.thumbnail
          img(src="/img/blackberry_logo.jpg", style="height:135px;")
      li.span3
        a.thumbnail
          img(src="/img/windows_mobile.jpg", style="height:135px;")

    paragraph = {headline:pageContent.headline, lines:pageContent.paragraphs}
    include paragraph

