doctype 5
html
  head
    title= title
    block stylesheets
      link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
      link(rel='stylesheet', href='/stylesheets/my.css')
    block scripts
      script(type='text/javascript', src='http://code.jquery.com/jquery.js')
